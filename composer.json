{
    "name": "zoopcommerce/gomi-module",
    "description": "Zend Framework 2 Module that provides user management for zoop's Shard",
    "type": "library",
    "keywords": [
        "zf2",
        "user",
        "zoop",
        "shard"
    ],
    "homepage": "http://zoopcommerce.github.io/gomi-module",
    "authors": [
        {
            "name": "Tim Roediger",
            "email": "superdweebie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zoopcommerce/shard-module"          : "~3.1",
        "doctrine/doctrine-mongo-odm-module" : "1.0.x-dev",
        "doctrine/doctrine-module"           : "1.0.x-dev as 1.0",
        "doctrine/common"                    : "2.4.x-dev as 2.4",
        "doctrine/mongodb-odm"               : "dev-master as 1.0.0-BETA9",
        "zendframework/zendframework"        : "2.3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7",
        "squizlabs/php_codesniffer": "1.4.*"
    },
    "autoload": {
        "psr-0": {
            "Zoop\\GomiModule": "src/"
        }
    }
}
